{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "src",
    "paths": {
      "pages/*": ["pages/*"],
      "components/*": ["components/*"],
      "firebaseApp/*": ["firebaseApp/*"]
    },
    "allowUmdGlobalAccess": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react",
    "skipLibCheck": true
  }
}
